a=int(input())
b=int(input())
c=int(input())
if a<b and a<c:
  if b>c:
    temp=b
    b=c
    c=temp
 elif b<a and b<c:
  if a<c:
    temp=b
    b=a
    a=temp
  else:
    temp=b
    b=a
    a=c
    c=temp
elif c<a and c<b:
  if a<b:
    temp=b
    b=a
    a=c
    c=temp
  else:
    temp=a
    a=c
    c=temp
print(a,b,c)
    
